{
  "name": "way-cli",
  "version": "2.0.0",
  "description": "A tool to help terminal users find their way",
  "main": "dist/main.js",
  "module": "dist/main.es.js",
  "jsnext:main": "dist/main.es.js",
  "preferGlobal": true,
  "bin": {
    "way": "dist/main.js"
  },
  "scripts": {
    "build": "npm run clean:build && rollup -c",
    "clean:build": "rm -rf dist",
    "dev": "npm run watch:ts",
    "start": "node dist/main.js",
    "test": "jest",
    "watch:ts": "tsc -w"
  },
  "author": "Dai Nguyendo",
  "license": "MIT",
  "dependencies": {
    "@google/maps": "^1.0.2",
    "chalk": "^3.0.0",
    "cli-table": "^0.3.1",
    "commander": "^4.0.1",
    "dotenv": "^8.2.0",
    "inquirer": "^7.0.1",
    "node-fetch": "^2.6.0",
    "open": "^7.0.0",
    "query-string": "^6.9.0"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-typescript": "^2.0.1",
    "@types/cli-table": "^0.3.0",
    "@types/google__maps": "^0.5.8",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.21",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.8.0",
    "eslint-config-minimal": "^0.4.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rollup": "^1.27.13",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-terser": "^5.1.3",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  },
  "homepage": "https://github.com/dainguyendo/way-cli",
  "repository": "github:dainguyendo/way-cli",
  "bugs": {
    "url": "https://github.com/dainguyendo/way-cli/issues"
  },
  "keywords": [
    "javascript",
    "typescript",
    "cli",
    "nodejs",
    "googlemaps-api"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md,ts}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
