{
  "name": "way-cli",
  "version": "1.0.2",
  "description": "A tool to help terminal users find their way",
  "main": "main.js",
  "preferGlobal": true,
  "bin": "./main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm link",
    "postuninstall": "npm unlink",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "start": "node dist/main.js"
  },
  "author": "Dai Nguyendo",
  "license": "MIT",
  "dependencies": {
    "@google/maps": "^1.0.2",
    "chalk": "^3.0.0",
    "cli-table": "^0.3.1",
    "commander": "^3.0.2",
    "dotenv": "^8.2.0",
    "inquirer": "^7.0.0",
    "node-fetch": "^2.6.0",
    "open": "^7.0.0",
    "query-string": "^6.8.3"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.0",
    "@types/google__maps": "^0.5.8",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^12.11.7",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.5.1",
    "eslint-config-minimal": "^0.4.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  },
  "homepage": "https://github.com/dainguyendo/way-cli",
  "repository": "github:dainguyendo/way-cli",
  "bugs": {
    "url": "https://github.com/dainguyendo/way-cli/issues"
  },
  "keywords": [
    "javascript",
    "cli",
    "nodejs",
    "googlemaps-api"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md,ts}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
